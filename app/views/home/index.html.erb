    <main> <turbo-frame id="frame">
      <div id="ffc-calculator__wrap">
        <section id="ffc-header" class="container mobile-adjusted page-top mt-3">
          <h1 style="text-align: center; font-weight: bold;">VK Fake Follower checker</h1>
          <p>Get an influencer’s audience audit &amp; identify fake, dormant or suspect followers to evaluate an influencer’s audience quality.</p>
        </section>
        <section id="ffc-calculator" class="container mobile-adjusted">
          <div id="ffc-form" class="form-group">
            <input type="string" class="form-control" required="required" placeholder="Enter VK @username">
            <div class="d-flex justify-content-center mt-2">
              <button type="submit" class="btn btn-dark mt-2">
                <span>Check stats</span>
              </button>
            </div>
            <div data-controller='vkid' class="mt-3">
             <div id="VkIdSdkOneTap"></div>
             </div>
        </button>
             </div>
          </div>
          <div id="ffc-result" class="card row section shadow m-5">
            <div class="container ">
              <div class="row row-cols-2  ">
                <div class="col p-4">
                  
            <div class="col-md-12 profile-info text-center p-2 m-auto">
